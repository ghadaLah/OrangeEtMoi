<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackatour.reclamationdigitale">

    <application
        android:allowBackup="true"
        android:icon="@drawable/orangetmoi"
        android:label="@string/app_name"
        android:roundIcon="@drawable/orangetmoi"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:host="espace-client.orange.fr" android:pathPrefix="/reclamation" android:scheme="https" />
            </intent-filter>
        </activity>
    </application>

</manifest>